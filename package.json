    "openai": "^4.28.0",
    "dotenv": "^16.4.1",
    "@sendgrid/mail": "^8.1.0",
    "googleapis": "^132.0.0",
    "zod": "^3.22.4",
    "nanoid": "^5.0.5",
    "date-fns": "^3.3.1"
  },
